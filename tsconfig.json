{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react-jsx",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/contact": [
        "./src/features/contact/index.ts"
      ],
      "@/contact/*": [
        "./src/features/contact/*"
      ],
      "@/company": [
        "./src/features/company/index.ts"
      ],
      "@/company/*": [
        "./src/features/company/*"
      ],
      "@/leads": [
        "./src/features/leads/index.ts"
      ],
      "@/leads/*": [
        "./src/features/leads/*"
      ],
      "@/customers": [
        "./src/features/customers/index.ts"
      ],
      "@/customers/*": [
        "./src/features/customers/*"
      ],
      "@/projectType": [
        "./src/features/projectType/index.ts"
      ],
      "@/projectType/*": [
        "./src/features/projectType/*"
      ],
      "@/project": [
        "./src/features/project/index.ts"
      ],
      "@/project/*": [
        "./src/features/project/*"
      ],
      "@/reports": [
        "./src/features/reports/index.ts"
      ],
      "@/reports/*": [
        "./src/features/reports/*"
      ]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "**/*.mts",
    ".next/dev/types/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
